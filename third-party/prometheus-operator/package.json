{
  "name": "@soft-stech/prometheus-operator",
  "version": "4.3.1",
  "description": "Kubernetes prometheus-operator models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/third-party/prometheus-operator",
  "author": "Julien Bouquillon <julien@bouquillon.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "crd-generate && publish-scripts build",
    "prepack": "publish-scripts prepack"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "linkDirectory": true
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "prometheus",
    "prometheus-operator"
  ],
  "dependencies": {
    "@soft-stech/apimachinery": "workspace:^",
    "@soft-stech/base": "workspace:^",
    "@soft-stech/validate": "workspace:^",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@soft-stech/crd-generate": "workspace:^",
    "@soft-stech/publish-scripts": "workspace:^",
    "vitest": "^0.29.8"
  },
  "crd-generate": {
    "input": [
      "https://github.com/prometheus-operator/prometheus-operator/releases/download/v0.65.1/stripped-down-crds.yaml"
    ],
    "output": "./gen"
  }
}
