{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "tsc-multi.json",
    "tsconfig.json",
    "tsconfig.build.json"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "gen/**",
        "*.tsbuildinfo"
      ]
    }
  }
}
