{
  "name": "@soft-stech/gateway-api",
  "version": "0.5.3",
  "description": "Gateway API models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/third-party/gateway-api",
  "author": "Tommy Chen <tommy351@gmail.com",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "crd-generate && publish-scripts build",
    "prepack": "publish-scripts prepack"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "linkDirectory": true
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "gateway-api"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@soft-stech/apimachinery": "workspace:^",
    "@soft-stech/base": "workspace:^",
    "@soft-stech/validate": "workspace:^",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@soft-stech/crd-generate": "workspace:^",
    "@soft-stech/publish-scripts": "workspace:^",
    "vitest": "^0.29.8"
  },
  "crd-generate": {
    "input": [
      "https://github.com/kubernetes-sigs/gateway-api/releases/download/v0.6.2/standard-install.yaml",
      "https://github.com/kubernetes-sigs/gateway-api/releases/download/v1.0.0/standard-install.yaml",
      "https://github.com/kubernetes-sigs/gateway-api/releases/download/v1.0.0/experimental-install.yaml"
    ],
    "output": "./gen"
  }
}
