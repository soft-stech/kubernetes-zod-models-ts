{
  "name": "@soft-stech/envoy-gateway",
  "version": "0.2.2",
  "description": "Envoy Gateway models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/third-party/envoy-gateway",
  "author": "Leo Zhan <leozhantw@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsx scripts/download-crd.ts && crd-generate && publish-scripts build",
    "prepack": "publish-scripts prepack"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "linkDirectory": true
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "envoy-gateway"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@soft-stech/apimachinery": "workspace:^",
    "@soft-stech/base": "workspace:^",
    "@soft-stech/validate": "workspace:^",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@soft-stech/crd-generate": "workspace:^",
    "@soft-stech/publish-scripts": "workspace:^",
    "@soft-stech/read-input": "workspace:^",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.7.23",
    "js-yaml": "^4.1.0",
    "tsx": "^4.19.3",
    "typescript": "5.8.3",
    "vitest": "^0.29.8"
  },
  "crd-generate": {
    "input": [
      "./crds/crd.yaml"
    ],
    "output": "./gen"
  }
}
