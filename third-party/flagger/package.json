{
  "name": "@soft-stech/flagger",
  "version": "0.4.2",
  "description": "Flagger models",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/third-party/flagger",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "ts-node scripts/download-crd.ts && crd-generate && publish-scripts build",
    "prepack": "publish-scripts prepack"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "linkDirectory": true
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "flagger"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@soft-stech/apimachinery": "workspace:^",
    "@soft-stech/base": "workspace:^",
    "@soft-stech/validate": "workspace:^",
    "@swc/helpers": "^0.5.8"
  },
  "devDependencies": {
    "@soft-stech/crd-generate": "workspace:^",
    "@soft-stech/publish-scripts": "workspace:^",
    "@soft-stech/read-input": "workspace:^",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.7.23",
    "js-yaml": "^4.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vitest": "^0.29.8"
  },
  "crd-generate": {
    "input": [
      "./crds/crd.yaml"
    ],
    "output": "./gen"
  }
}
