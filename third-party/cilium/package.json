{
  "name": "@soft-stech/cilium",
  "version": "3.1.0",
  "description": "Cilium CRD",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/third-party/cilium",
  "author": "Rueian <rueiancsie@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "crd-generate && publish-scripts build",
    "prepack": "publish-scripts prepack"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "linkDirectory": true
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "cilium"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@soft-stech/apimachinery": "workspace:^",
    "@soft-stech/base": "workspace:^",
    "@soft-stech/validate": "workspace:^",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@soft-stech/crd-generate": "workspace:^",
    "@soft-stech/publish-scripts": "workspace:^",
    "vitest": "^0.29.8"
  },
  "crd-generate": {
    "input": [
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2/ciliumclusterwideenvoyconfigs.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2/ciliumclusterwidenetworkpolicies.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2/ciliumegressgatewaypolicies.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2/ciliumendpoints.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2/ciliumenvoyconfigs.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2/ciliumexternalworkloads.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2/ciliumidentities.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2/ciliumlocalredirectpolicies.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2/ciliumnetworkpolicies.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2/ciliumnodes.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2alpha1/ciliumbgppeeringpolicies.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2alpha1/ciliumcidrgroups.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2alpha1/ciliumendpointslices.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2alpha1/ciliuml2announcementpolicies.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2alpha1/ciliumloadbalancerippools.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2alpha1/ciliumnodeconfigs.yaml",
      "https://raw.githubusercontent.com/cilium/cilium/v1.14.4/pkg/k8s/apis/cilium.io/client/crds/v2alpha1/ciliumpodippools.yaml"
    ],
    "output": "./gen"
  }
}
